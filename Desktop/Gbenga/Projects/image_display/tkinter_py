from tkinter import *

root = Tk()

# creating a label widget
my_label = Label(root, text='welcome users')
my_label1 = Label(root, text='This application lets you view your pictures in slide show')


# displaying to screen 
my_label.grid(row=0, column=0) 
my_label1.grid(row=1, column=0)












root.mainloop() 